{
  "metadata": {
    "title": "The simplest model!"
  },
  "timestepper": {
    "start": "2020-01-01",
    "end": "2020-12-31",
    "timestep": 1
  },
  "nodes": [
    {
      "name": "input1",
      "type": "Input",
      "tag": "eastern-area"
    },
    {
      "name": "link1",
      "type": "Link"
    },
    {
      "name": "output1",
      "type": "Output",
      "cost": -10.0,
      "max_flow": {
        "type": "Parameter",
        "name": "demand"
      }
    }
  ],
  "edges": [
    {
      "from_node": "input1",
      "to_node": "link1"
    },
    {
      "from_node": "link1",
      "to_node": "output1"
    }
  ],
  "parameters": [
    {
      "name": "demand",
      "type": "Python",
      "path": "my/custom/param.py",
      "object": "MyCustomParameter"
    }
  ],
  "outputs": [
    {
      "format": "CSV",
      "url": "outputs.csv",
      "metrics": [
        {
          "type": "Parameter",
          "name": "demand"
        },
        {
          "type": "NodeVolume",
          "name": "my-reservoir"
        },
        {
          "type": "NodeProportionalVolume",
          "name": "my-other-reservoir"
        },


        {
          "attribute": "volume",  // Come back to this
          "name": "my-reservoir"
        },
        {
          "attribute": "volume",  // Come back to this
          "name": "my-reservoir"
        }
      ]
    },
    {
      "format": "CSV",
      "url": "annual-outputs.csv",
      "aggregation": {
        "frequency": "Annual(start_month, start_day) | Simulation/null | Monthly | Event(event_definition)",
        "funcs": ["min", "max", "sum", "mean", "median", "quantile"]
      },
      "metrics": [
        {
          "type": "Parameter",
          "name": "demand"
        },
        {
          "type": "NodeVolume",
          "name": "my-reservoir"
        },
        {
          "type": "NodeProportionalVolume",
          "name": "my-other-reservoir"
        },
        {
          "type": "IndexParameter",
          "name": "my-failure-index",
          "funcs": [
            {
              "type": "Count",
              "predicate": ">=",
              "threshold": 1
            }
          ]
        }
      ]
    }
  ]

}
